  Arr: TDynArrayOfByte;
begin
  InitDynArrayToEmpty(Arr);
  SetDynLength(Arr, 3);
  Arr.Content^[0] := 30;
  Arr.Content^[1] := 40;
  Arr.Content^[2] := 50;

  RemoveStartBytesFromDynArray(0, Arr);

  ExpectDWord(Arr.Len).ToBe(3, '');
  ExpectDWord(Arr.Content^[0]).ToBe(30, '');
  ExpectDWord(Arr.Content^[1]).ToBe(40, '');
  ExpectDWord(Arr.Content^[2]).ToBe(50, '');

  FreeDynArray(Arr);
end;


